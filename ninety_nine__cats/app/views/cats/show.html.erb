<h1>One cat!</h1>

<table>
    <thead>
        <tr>
            <th>
                <%= @cat.name %>
            </th>
        </tr>
    </thead>

    <tbody>
        <tr>
            <td>
                <%= @cat.birth_date %>
            </td>
        </tr>
        <tr>
            <td>
                <%= @cat.color %>
            </td>
        </tr>
        <tr>
            <td>
                <%= @cat.sex %>
            </td>
        </tr>
        <tr>
            <td>
                <%= @cat.description %>
            </td>
        </tr>
        </tr>
    </tbody>

</table>
<a href=" <%= cats_url %>">cats</a>