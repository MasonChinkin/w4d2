<h1>All the cats!</h1>

<p><a href=" <%= new_cat_url %>">create a cat</a></p>

<ul>
    <%@cats.each do |cat|%>
        <li>
            <%= cat.name %>
           <ul>
               <li>
                   <a href=" <%= cat_url(cat) %>">cat details</a>
               </li>
               <li>
                   <a href=" <%= edit_cat_url(cat) %>">experiment on cat</a>
               </li>
           </ul>
        </li>
    <%end%>
</ul>
